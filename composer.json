{
    "name": "thorsten/phpMyFAQ",
    "description": "FAQ System",
    "keywords": [
        "phpMyFAQ",
        "FAQ"
    ],
    "homepage": "http://www.phpmyfaq.de",
    "license": "MPL 2.0",
    "type": "application",
    "version": "3.0.0-alpha",
    "authors": [
        {
            "name": "Thorsten Rinne",
            "email": "thorsten@phpmyfaq.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "tcpdf",
                "version": "master",
                "source": {
                    "url": "http://git.code.sf.net/p/tcpdf/code",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "thorsten/twitteroauth",
                "version": "master",
                "source": {
                    "url": "https://github.com/thorsten/twitteroauth.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "phpseclib": "master@dev",
        "swiftmailer/swiftmailer": "5.4.8",
        "erusev/parsedown": "1.6.3",
        "erusev/parsedown-extra": "~0.7.0",
        "tecnickcom/tcpdf": "6.2.13",
        "elasticsearch/elasticsearch": "~2.0",
        "monolog/monolog": "1.23.0",
        "abraham/twitteroauth": "^0.7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "phpunit/dbunit": "1.3.*",
        "doctrine/instantiator": "1.0.*",
        "symfony/yaml": "3.*",
        "phpdocumentor/reflection-docblock": "3.*",
        "phpunit/php-token-stream": "1.*"
    },
    "config": {
        "bin-dir": "bin",
        "secure-http": false
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "scripts/moveVendors.sh"
        ],
        "post-update-cmd": [
            "scripts/moveVendors.sh"
        ]
    }
}
