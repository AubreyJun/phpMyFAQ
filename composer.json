{
    "name": "thorsten/phpMyFAQ",
    "description": "FAQ System",
    "keywords": [
        "phpMyFAQ",
        "FAQ"
    ],
    "homepage": "http://www.phpmyfaq.de",
    "license": "MPL-2.0",
    "type": "application",
    "authors": [
        {
            "name": "Thorsten Rinne",
            "email": "thorsten@phpmyfaq.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "tcpdf",
                "version": "master",
                "source": {
                    "url": "http://git.code.sf.net/p/tcpdf/code",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/class-loader": "2.6.*",
        "phpseclib/phpseclib": "2.0.*",
        "abraham/twitteroauth": "0.6.6",
        "swiftmailer/swiftmailer": "5.4.9",
        "erusev/parsedown": "~1.5.2",
        "erusev/parsedown-extra": "~0.7.0",
        "tecnickcom/tcpdf": "^6.2",
        "elasticsearch/elasticsearch": "~2.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "1.3.*",
        "doctrine/instantiator": "1.0.*",
        "symfony/yaml": "3.*",
        "phpdocumentor/reflection-docblock": "2.*",
        "phpunit/php-token-stream": "1.*"
    },
    "config": {
        "bin-dir": "bin",
        "secure-http": false
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "scripts/moveVendors.sh"
        ],
        "post-update-cmd": [
            "scripts/moveVendors.sh"
        ]
    }
}